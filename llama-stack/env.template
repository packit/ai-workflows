# Llama Stack Jira Issue Analysis Agent - Environment Variables
# Copy this file to .env and fill in your actual values

# ========================================
# Gemini Configuration
# ========================================
GOOGLE_API_TOKEN=your_google_api_token

# ========================================
# Jira Configuration
# ========================================
# Personal token for Jira API access
JIRA_PERSONAL_TOKEN=your_jira_personal_token_here

# Your Jira instance URL (without trailing slash)
JIRA_URL=https://issues.redhat.com

# ========================================
# Model Configuration
# ========================================
INFERENCE_MODEL=gemini/gemini-2.5-pro

# ========================================
# Optional Configuration
# ========================================
# Enable debug logging (uncomment to enable)
# LLAMA_STACK_LOG_LEVEL=DEBUG

# Llama Stack server URL for agent connections
LLAMA_STACK_URL=http://llama-stack:8321 